<h1>Existing Business Services:</h1>
<a href="#" onclick="displayService(); return false;">Display Business Services</a>
<%# <a href="#" onclick="getTodos(); return false;">Todos</a> %>

<div id="info">

<ol>
<% @business_services.each do |service| %>
  <% if @user.id == service.user_id %>
      <li> Service: <%= service.name %><br>
      Description: <%= service.description %><br>
      <%= link_to "See More", business_service_path(service) %> </li>
      <br>
  <% end %>
<% end %>
</ol>




